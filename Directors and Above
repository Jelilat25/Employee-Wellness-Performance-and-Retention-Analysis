    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Contents of /kaggle/working directory:\n",
      "['__notebook__.ipynb', 'correlation_heatmap.png', 'correlation_with_heatmap.xlsx']\n",
      "File created successfully: /kaggle/working/correlation_with_heatmap.xlsx\n"
